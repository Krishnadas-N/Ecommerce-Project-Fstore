                        </li>
                    </ul>
                    <a href="#" class="view-more d-none d-md-flex">View More<i class="fi-rs-angle-double-small-right"></i></a>
                </div>
                <!--End nav-tabs-->

               
                
                
                  
                <div class="tab-content wow fadeIn animated" id="myTabContent">
                    <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">

                        
                        <div class="row product-grid-4">

                            <% products?.forEach((element,index) => { %>
                                <% if(element.isFeatured){ %>
                            <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                                <div class="product-cart-wrap mb-30" style="height: 95%;">
                                    <div class="product-img-action-wrap">
                                        <div class="product-img product-img-zoom">
                                            <a href="/product-detail/<%= element._id  %>">
                                                <img class="default-img" src="/<%= element.image %>" alt="" style=" height: 250px;">
                                                <img class="hover-img" src="/<%= element.images[0]  %>" alt="">
                                            </a>
                                        </div>
                                        <!-- <div class="product-action-1">
                                            <a aria-label="Quick view" class="action-btn hover-up" data-bs-toggle="modal" data-bs-target="#quickViewModal"><i class="fi-rs-eye"></i></a>
                                            <a aria-label="Add To Wishlist" class="action-btn hover-up" href="shop-wishlist.html"><i class="fi-rs-heart"></i></a>
                                            <a aria-label="Compare" class="action-btn hover-up" href="shop-compare.html"><i class="fi-rs-shuffle"></i></a>
                                        </div> -->
                                        <div class="product-badges product-badges-position product-badges-mrg">
                                            <span class="hot">Hot</span>
                                        </div>
                                    </div>
                                    <div class="product-content-wrap">
                                        <div class="product-category">
                                            <a href="#"><%= element.category.name %></a>
                                        </div>
                                        <h2><a href="/product-detail/<%= element._id  %>"><%= element.name  %></a></h2>
                                        <div class="d-flex">
                                            <% for (let i = 1; i <= 5; i++) { %>
                                                <% if(i<=element.rating){ %>
                                                    <i class="fa fa-star" style="color: yellow; margin-top: 3px;"></i>
                                                    <% }else{ %>
                                                        <i class="fa fa-star" style="color: black; border-color: black; margin-top: 3px;"></i>
                                                        <% } } %>     <span class="ms-2"><%= element.rating %></span> 
                                                   
                                            

                                               
                                        </div>
                                        <div class="product-price">
                                            <span>INR <%= element.price  %></span>
                                            <span class="old-price"><%= element.price +1500 %></span>
                                        </div>
                                        <!-- <div class="product-action-1 show">
                                            <a aria-label="Add To Cart" class="action-btn hover-up" href="#"><i class="fi-rs-shopping-bag-add"></i></a>
